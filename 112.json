{
  "chapter": [
    {
      "verse": 1,
      "arab": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
      "latin": "bismillaahir-rohmaanir-rohiim",
      "id": "Dengan menyebut nama Allah Yang Maha Pengasih lagi Maha Penyayang."
    },
    {
      "verse": 2,
      "arab": "قُلْ هُوَ اللَّهُ أَحَدٌ",
      "latin": "qul huwallaahu ahad",
      "id": "Katakanlah (Muhammad), 'Dialah Allah, Yang Maha Esa.'"
    },
    {
      "verse": 3,
      "arab": "اللَّهُ الصَّمَدُ",
      "latin": "allaahush-shamad",
      "id": "Allah tempat meminta segala sesuatu."
    },
    {
      "verse": 4,
      "arab": "لَمْ يَلِدْ وَلَمْ يُولَدْ",
      "latin": "lam yalid wa lam yuulad",
      "id": "Dia tidak beranak dan tidak pula diperanakkan."
    },
    {
      "verse": 5,
      "arab": "وَلَمْ يَكُن لَّهُ كُفُوًا أَحَدٌ",
      "latin": "wa lam yakun lahu kufuwan ahad",
      "id": "Dan tidak ada sesuatu yang setara dengan Dia."
    }
  ]
}
